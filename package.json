  },
  "dependencies": {
    "@types/fs-extra": "^5.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^7.0.0",
    "jquery": "^3.3.1",
    "sequelize": "^4.39.0",
    "sharp": "^0.20.8",
    "sqlite3": "^4.0.2",
    "win-explorer": "file:..\\local_modules\\win-explorer",
    "zip-local": "^0.3.4"
