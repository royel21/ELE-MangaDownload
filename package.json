  },
  "dependencies": {
    "@types/fs-extra": "^5.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^7.0.0",
    "jquery": "^3.4.1",
    "sequelize": "^4.44.3",
    "sharp": "^0.23.2",
    "sqlite3": "^4.1.0",
    "win-explorer": "file:..\\local_modules\\win-explorer",
    "zip-local": "^0.3.4"
