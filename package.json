{
  "name": "mangadownload",
  "version": "0.0.1",
  "description": "A Manga Downloader",
  "build": {
    "appId": "rc-manga-download"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-package": "electron-packager . --platform=win32 --arch=x64 MangaDownload",
